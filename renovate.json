{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "branchName": "{{#if (equals depName \"actions/checkout\"}}REFS/PULL/20/merge{{/if}}{{#unless (equals depName \"actions/checkout\"}}IGNOREME{{/unless}}",
  "labels": [
    "renovatebot",
    "dependencies"
  ],
  "constraints": {
    "go": "1.23"
  },
  "extends": [
    "config:recommended",
    "helpers:pinGitHubActionDigests"
  ],
  "schedule": [
    "on tuesday"
  ],
  "packageRules": [
    {
      "matchManagers": [
        "github-actions"
      ],
      "groupName": "github-actions deps"
    },
    {
      "matchManagers": [
        "gomod"
      ],
      "enabled": false
    }
  ],
  "prConcurrentLimit": 200,
  "suppressNotifications": [
    "prEditedNotification"
  ]
}
